export PATH=${PATH}:/usr/local/mysql/bin
export PATH=${PATH}:/Users/lukasadmin/Library/Android/sdk/platform-tools
export PATH=${PATH}:$HOME/.node/bin
export PATH=${PATH}:/usr/local/bin
export PATH=${PATH}:/usr/local/bin/liquibase-3.5.1-bin
