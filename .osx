export PATH=${PATH}:/Users/lukasadmin/Library/Android/sdk/build-tools/24.0.1
export CLASSPATH=/usr/local/bin/mysql-connector-java-5.1.39/mysql-connector-java-5.1.39-bin.jar:$CLASSPATH


### iTerm ###
if [ -f ~/dotfiles/lib/iterm2_shell_integration.bash ]; then
