#!/bin/bash

### colors ###
RED='\033[0;31m'
GRN='\033[0;32m'
YEL='\033[33m'
CYN='\033[36m'
LGR='\033[37m'
DGR='\033[90m'
WHT='\033[97m'
NC='\033[0m' # No Color

# osx alias
alias ds='sudo ~/dotfiles/lib/deepsleep'

### Path exports ###

export PATH=${PATH}:/usr/local/mysql/bin
export PATH=${PATH}:$HOME/Library/Android/sdk/platform-tools
export PATH=${PATH}:$HOME/.node/bin
export PATH=${PATH}:/usr/bin
export PATH=${PATH}:/usr/sbin
export PATH=${PATH}:/sbin
export PATH=${PATH}:/usr/local/bin
export PATH=${PATH}:/usr/local/bin/liquibase-3.5.1-bin
export PATH=${PATH}:/usr/local/bin/redis-3.2.0/src
export PATH=${PATH}:$HOME/Library/Android/sdk/build-tools/24.0.1
export PATH=${PATH}:/Library/Frameworks/Python.framework/Versions/3.5/bin
export CLASSPATH=/usr/local/bin/mysql-connector-java-5.1.39/mysql-connector-java-5.1.39-bin.jar:$CLASSPATH
export ANDROID_HOME=$HOME/Library/Android/sdk

### iTerm ###
if [ -f ~/dotfiles/lib/iterm2_shell_integration.bash ]; then
    source ~/dotfiles/lib/iterm2_shell_integration.bash
fi

clear
